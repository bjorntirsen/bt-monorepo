{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {},
    "test": {},
    "check:types": {},
    "format": {},
    "format:check": {},
    "test:browser-based": {},
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
